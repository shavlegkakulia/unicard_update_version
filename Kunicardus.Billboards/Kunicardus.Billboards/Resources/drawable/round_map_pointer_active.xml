<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

  <item>
    <shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval" >
      <corners
          android:radius="5dp" />
      <solid
          android:color="#E95936" />
      <stroke
          android:width="3dp"
          android:color="#ffffff" />
      <size
           android:width="20dp"
            android:height="20dp"/>
    </shape>
  </item>
  <item android:left="10dp" android:top="10dp" android:right="10dp" android:bottom="10dp">
    <shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval" >
      <corners
          android:radius="5dp" />
      <solid
          android:color="#ffffff" /> />
      <size
           android:width="13dp"
            android:height="13dp"/>
    </shape>
  </item>
</layer-list>