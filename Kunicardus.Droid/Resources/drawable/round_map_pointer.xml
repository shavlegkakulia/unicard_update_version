<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

  <item>
    <shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval" >
      <corners
          android:radius="5dp" />
      <solid
          android:color="#8dbd3b" />
      <stroke
          android:width="3dp"
          android:color="#ffffff" />
      <size
           android:width="32dp"
            android:height="32dp"/>
    </shape>
  </item>
  <item android:left="10dp" android:top="10dp" android:right="10dp" android:bottom="10dp">
    <shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval" >
      <corners
          android:radius="5dp" />
      <solid
          android:color="#ffffff" /> />
      <size
           android:width="6dp"
            android:height="6dp"/>
    </shape>
  </item>
</layer-list>